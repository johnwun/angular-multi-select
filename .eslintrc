{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "ecmaFeatures": {
    "modules": true,
    "templateStrings": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "binaryLiterals": false,
    "classes": false,
    "defaultParams": false,
    "destructuring": false,
    "forOf": false,
    "generators": false,
    "objectLiteralComputedProperties": false,
    "objectLiteralDuplicateProperties": false,
    "objectLiteralShorthandMethods": false,
    "objectLiteralShorthandProperties": false,
    "octalLiterals": false,
    "regexUFlag": false,
    "regexYFlag": false,
    "restParams": false,
    "spread": false,
    "superInFunctions": false,
    "unicodeCodePointEscapes": false,
    "globalReturn": false,
    "jsx": false,
    "experimentalObjectRestSpread": false
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jasmine": true,
    "protractor": true,
    "jquery": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "no-cond-assign": 2,
    "no-control-regex": 2,
    "no-constant-condition": 1,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 0,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 1,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,


    "accessor-pairs": 2,
    "block-scoped-var": 2,
    "consistent-return": 2,
    "curly": 2,
    "default-case": 2,
    "dot-notation": 2,
    "dot-location": [2, "property"],
    "eqeqeq": [2, "smart"],
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 2,
    "no-else-return": 1,
    "no-empty-label": 2,
    "no-eq-null": 1,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": [2, {"boolean": false, "number": true, "string": true}],
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 0,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-void": 2,
    "no-warning-comments": 1,
    "no-with": 2,
    "radix": 2,
    "wrap-iife": [2, "inside"],
    "yoda": 2,
    "strict": 0,

    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [2, "nofunc"],
    "array-bracket-spacing": 0,
    "block-spacing": 0,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "comma-spacing": [0, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "consistent-this": [2, "self"],
    "eol-last": 2,
    "func-names": 0,
    "func-style": [2, "declaration", { "allowArrowFunctions": true }],
    "indent": [2, 2],
    "key-spacing": [0, {"beforeColon": false, "afterColon": true}],
    "linebreak-style": 2,
    "new-cap": 0,
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-continue": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-nested-ternary": 0,
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "object-curly-spacing": 0,
    "one-var": [2, "never"],
    "operator-assignment": [0, "never"],
    "quotes": [2, "single"],
    "semi-spacing": [2, {"before": false, "after": true}],
    "semi": [2, "always"],
    "space-return-throw-case": 2,
    "space-before-function-paren": [2, "never"],
    "space-before-blocks": 2,
    "space-after-keywords": 2,
    "space-before-keywords": 2,
    "space-infix-ops": 2,
    "space-unary-ops": [2, {"words": true, "nonwords": false}],
    "spaced-comment": [0, "always"],

    "react/display-name": 1,
    "react/forbid-prop-types": 0,
    "react/jsx-boolean-value": 2,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-curly-spacing": 0,
    "react/jsx-equals-spacing": 0,
    "react/jsx-handler-names": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-indent": 0,
    "react/jsx-key": 2,
    "react/jsx-max-props-per-line": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-literals": 0,
    "react/jsx-no-undef": 2,
    "react/jsx-pascal-case": 0,
    "react/jsx-quotes": 0,
    "react/jsx-sort-prop-types": 0,
    "react/jsx-sort-props": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-danger": 0,
    "react/no-deprecated": 2,
    "react/no-did-mount-set-state": 0,
    "react/no-did-update-set-state": 0,
    "react/no-direct-mutation-state": 0,
    "react/no-is-mounted": 2,
    "react/no-multi-comp": 0,
    "react/no-set-state": 0,
    "react/no-string-refs": 2,
    "react/no-unknown-property": 0,
    "react/prefer-es6-class": 2,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 2,
    "react/require-extension": 0,
    "react/self-closing-comp": 0,
    "react/sort-comp": 0,
    "react/wrap-multilines": 0
  },
  "globals": {
    "angular": false,
    "$": false,
    "_": false,
    "jQuery": false,
    "nux": false,
    "d3": false,
    "moment": false,
    "httpStatusCode": false,

    "expect": false,
    "describe": false,
    "it": false,
    "inject": false,
    "module": false,
    "beforeEach": false,
    "afterEach": false,
    "jest": false
  }
}
